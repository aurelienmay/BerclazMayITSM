<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICalculator1" />
                <binding name="BasicHttpBinding_IPaymentService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:50566/Calculator.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_ICalculator1" contract="ServiceReferenceCalculator.ICalculator"
                name="BasicHttpBinding_ICalculator1" />
            <endpoint address="http://localhost:56668/PaymentService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IPaymentService"
                contract="PaymentService.IPaymentService" name="BasicHttpBinding_IPaymentService" />
        </client>
    </system.serviceModel>
